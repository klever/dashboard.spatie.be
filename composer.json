{
    "name": "spatie/laravel-dashboard",
    "description": "Spatie's Laravel Dashboard package.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "barryvdh/laravel-ide-helper": "^2.1",
        "erusev/parsedown": "^1.6",
        "fennb/phirehose": "^1.0",
        "guzzlehttp/guzzle": "^6.2",
        "knplabs/github-api": "^2.4",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "pda/pheanstalk": "^3.1",
        "php-http/guzzle6-adapter": "^1.1",
        "pusher/pusher-php-server": "^2.3",
        "spatie/laravel-google-calendar": "^2.0",
        "spatie/laravel-tail": "^1.1",
        "spatie/laravel-twitter-streaming-api": "^0.0.2",
        "spatie/last-fm-now-playing": "^1.0",
        "spatie/packagist-api": "^1.0.1",
        "spatie/valuestore": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "orchestra/testbench": "~3.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Spatie\\LaravelDashboard\\": "src/",
            "Tests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "laravel-echo-server": "laravel-echo-server start"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
